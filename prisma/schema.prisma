generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Place {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  data Json
}

model User {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  access_code String
  discord_id  String
  name        String
}